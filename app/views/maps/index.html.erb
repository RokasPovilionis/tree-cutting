 src = "https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyBKgjgdGIMcQh9jaXKxFGJFWVGfmAlFQ_Y">
<%
=end%>
 src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBKgjgdGIMcQh9jaXKxFGJFWVGfmAlFQ_Y&callback=initMap">
      <%
